package methods;

public class MathTest2 {

	public static void main(String[] args) {
		for(int i=1; i<=10; i++) {
			int dice =(int)(Math.random()*6)+1;	
			System.out.println(dice);
		}
		//배열에서 문자열 랜덤 추출
		String[] word = {"나","너","우리","나라"};
		int idx=(int)(Math.random()*4); //0~3
		System.out.println(word[idx]);;
				
		}
		

	}

}
