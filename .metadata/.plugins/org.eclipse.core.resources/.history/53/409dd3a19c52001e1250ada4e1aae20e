package kr.or.iei.run;

import kr.or.iei.model.User;

public class UserTest {

	public static void main(String[] args) {
	//매개변수가 있는 생성자로 user1 인스턴스 생성
	User user0 = new User("sky789", "12345", "서정원");
	
	System.out.println("아이디:"+user0.getId());
	System.out.println("비밀번호:"+user0.getPwd());
	System.out.println("이름:"+user0.getName());
		
		 User[] users = new User[3];
	 User user1 = new User("sky789", "12345", "서정원");
	 User user2 = new User("sky789", "12345", "서정원");
	 User user3 = new User("sky789", "12345", "서정원");
		
	 //배열에 객체를 저장(입력)
	 users[0]=user1;
	 users[1]=user2;
	 users[2]=user3;
	//users[0] 1개출력
	 System.out.println(users[0].getId()+users[0].getPwd()+users[0].getName());
		 
	}

}
