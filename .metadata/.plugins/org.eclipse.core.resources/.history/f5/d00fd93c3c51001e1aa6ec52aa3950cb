package inheritance.statement;

public class Drink {
	private String name; //상품명 
	private int price;   //가격
	private int count;   //수량

	public Drink(String name, int price, int count) {
		this.name = name;
		this.price = price;
		this.count =count;
		
	}
	
	public void printTitle() {//제목행 출력
		System.out.println("상품명\t가격\t수량\t금액");
		
	}
}
