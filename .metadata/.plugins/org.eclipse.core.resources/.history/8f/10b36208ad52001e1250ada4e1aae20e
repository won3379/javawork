package kr.or.iei.model;

public class User1 {
	
	private String id;
	private String pwd;
	private String name;
	
	public User() {}
	
	public User(String id, String pwd, String name) {
		this.id = id;
		this.pwd = pwd;
		this.name = name;
	}
	
	//set+필드 이름 ()-입력,
	//, get + 필드이름 ()함수 -출력
	public void setId(String id) {
		this.id=id;	
	}
	public void setPwd(String id) {
		this.pwd=pwd;	
	}
	public void setName(String id) {
		this.name=name;	
	}
	
	}

}
